<h1>New Bookmark</h1>

<%= form_with model: @bookmark, local: true do |form| %>

  <div>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.text_field :url_link, placeholder: "Url" %>
  </div>

  <div>
    <%= form.text_field :description %>
  </div>
  
  <div>
    <%= form.submit 'Create' %>
  </div>

  

<% end %>
<%= link_to 'Back', bookmarks_path %>
